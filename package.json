      "@aws-sdk/client-location": "^3.540.0",
      "@aws-sdk/client-rds": "^3.516.0",
      "@aws-sdk/client-rds-data": "^3.514.0",
      "@aws-sdk/client-s3": "^3.504.0",
      "@aws-sdk/client-secrets-manager": "^3.515.0",
      "@aws-sdk/client-sqs": "^3.547.0",
      "@aws-sdk/lib-dynamodb": "^3.504.0",
