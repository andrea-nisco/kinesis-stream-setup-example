      "convert-csv-to-json": "^2.44.0",
      "date-fns": "^3.3.1",
      "del-cli": "^5.1.0",
      "esbuild": "^0.20.0",
      "eslint": "^8.56.0",
      "eslint-plugin-better-mutation": "^1.4.1",
      "eslint-plugin-etc": "^2.0.3",
      "eslint-plugin-jest": "^27.6.3",
      "eslint-plugin-unicorn": "^51.0.1",
      "firebase-admin": "^11.11.1",
