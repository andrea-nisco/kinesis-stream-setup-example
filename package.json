    "description": "KINESIS Backend",
    "main": "index.js",
    "scripts": {
      "lint": "eslint --fix --ext .ts .",
      "test": "jest -c jest.config.js",
      "build:all:src": "npm run build:lambda:consumer && npm run build:lambda:api-event-trigger",
      "prebuild:all:src": "del-cli --force ./dist",
